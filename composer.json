{
    "name": "angelolockdev/laravel-collective-to-spatie-laravel-migrator",
    "description": "A simple tool to migrate Laravel Collective HTML syntax to Spatie Laravel HTML syntax.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gemini",
            "email": "gemini@google.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Fddo\\LaravelHtmlMigrator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fddo\\LaravelHtmlMigrator\\HtmlMigratorServiceProvider"
            ]
        }
    }
}
